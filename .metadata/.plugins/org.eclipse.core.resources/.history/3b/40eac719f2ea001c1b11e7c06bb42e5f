package for_;

import java.util.Scanner;

public class star {

	public static void main(String[] args) {
		//스캐너사용 입력받은 값만크 별찍기
		Scanner sc = new Scanner(System.in);
		int result = sc.nextInt();
		System.out.println("출력할 별의 갯수 : " + result);
		
		for(int i=0; i<=result; i++) {
			System.out.print("*");
		}
	}

}
