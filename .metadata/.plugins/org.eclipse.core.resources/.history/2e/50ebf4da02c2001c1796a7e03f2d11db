package exam;

public class Exam0 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] arr=new int[10];
		
		for(int i=0; i<arr.length; i++) {
			arr[i]=i;
		}
		for(int i=0; i<11; i++) {
			int tmp = arr[0];
			int n =(int)Math.random()*10;
			arr[0] = arr[n];
			arr[n]=tmp;
		}
		for(int i=0; i<100; i++) {
			System.out.println(arr[i]);
		}

	}

}
